// BOY OR GIRL


#include<iostream>
#include<cstring>
#include<algorithm>
using namespace std;
int main(){

int count;
string s;
cin>>s;
sort(s.begin(),s.end ());
for (int i=0;i<s.size();i++){

    if (s[i]!=s[i+1])
        count++;
}

if (count%2==0)
    cout<<"CHAT WITH HER !"<<endl;

else
    cout<<"IGNORE HIM!";


    return 0;

}
